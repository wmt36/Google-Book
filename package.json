  "bugs": {
    "url": "https://github.com/wmt36/Google-Book/issues"
  },
  "homepage": "https://github.com/wmt36/Google-Book#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongod": "^2.0.0",
    "mongojs": "^3.1.0",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "path": "^0.12.7",
    "proxy": "^1.0.2",
    "seed": "^0.4.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
